DEPFLAGS := $(if $(word 2, $(TARGET_ARCH)), , -MMD)

ifeq ($(CONFIG),Debug)
  BINDIR := ../../../../../../bin
  LIBDIR := ../../../../../../lib
  OBJDIR := obj/Debug
  OUTDIR := ../../../../../../bin
  CPPFLAGS := $(DEPFLAGS) -D "_CRT_SECURE_NO_DEPRECATE" -D "HAVE_CONFIG_H" -D "TOLUA_EXPORTS" -D "_DEBUG" -I "../../../../../../cegui/include" -I "../../../../../../dependencies/include" -I "../../../../../../cegui/include/ScriptingModules/LuaScriptModule/support/tolua++/"
  CFLAGS += $(CPPFLAGS) $(TARGET_ARCH) -g
  CXXFLAGS += $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) -shared -Wl,--out-implib="../../../../../../lib/libtolua++_d.a" -L"../../../../../../dependencies/lib/dynamic" -llua_d
  LDDEPS :=
  RESFLAGS := -D "_CRT_SECURE_NO_DEPRECATE" -D "HAVE_CONFIG_H" -D "TOLUA_EXPORTS" -D "_DEBUG" -I "../../../../../../cegui/include" -I "../../../../../../dependencies/include" -I "../../../../../../cegui/include/ScriptingModules/LuaScriptModule/support/tolua++/"
  TARGET := tolua++_d.dll
 BLDCMD = $(CC) -o $(OUTDIR)/$(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES) $(TARGET_ARCH)
