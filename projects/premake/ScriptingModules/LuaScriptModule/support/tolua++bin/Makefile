ifndef CONFIG
  CONFIG=Debug
endif

# if multiple archs are defined turn off automated dependency generation
DEPFLAGS := $(if $(word 2, $(TARGET_ARCH)), , -MMD)

ifeq ($(CONFIG),Debug)
  BINDIR := ../../../../../../bin
  LIBDIR := ../../../../../../lib
  OBJDIR := obj/Debug
  OUTDIR := ../../../../../../bin
  CPPFLAGS := $(DEPFLAGS) -D "_CRT_SECURE_NO_DEPRECATE" -D "HAVE_CONFIG_H" -D "_DEBUG" -I "../../../../../../cegui/include" -I "../../../../../../dependencies/include" -I "../../../../../../cegui/include/ScriptingModules/LuaScriptModule/support/tolua++bin/../tolua++"
  CFLAGS += $(CPPFLAGS) $(TARGET_ARCH) -g
  CXXFLAGS += $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) -L"../../../../../../dependencies/lib/dynamic" -ltolua++_d -llua_d
  LDDEPS := ../../../../../../bin/tolua++_d.dll
  RESFLAGS := -D "_CRT_SECURE_NO_DEPRECATE" -D "HAVE_CONFIG_H" -D "_DEBUG" -I "../../../../../../cegui/include" -I "../../../../../../dependencies/include" -I "../../../../../../cegui/include/ScriptingModules/LuaScriptModule/support/tolua++bin/../tolua++"
  TARGET := tolua++cegui_d.exe
 BLDCMD = $(CC) -o $(OUTDIR)/$(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES) $(TARGET_ARCH)
endif

